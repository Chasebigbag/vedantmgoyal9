{
  "$schema": "https://raw.githubusercontent.com/vedantmgoyal2009/winget-pkgs-automation/main/schema.json",
  "pkgid": "BraveSoftware.BraveBrowser-Dev",
  "repo": "brave/brave-browser",
  "last_checked_tag": "v1.31.60$result = $(Invoke-WebRequest -Headers $header -Uri "https://api.github.com/repos/$($package.repo)/releases?per_page=1" -UseBasicParsing -Method Get | ConvertFrom-Json)[0] | Select-Object -Property name,tag_name,assets -First 1
if ($result.tag_name -gt $package.last_checked_tag -and $result.name.Contains("Beta"))
{
    $update_found = $true
    $version = "94.$($result.tag_name)"
    $jsonTag = $result.tag_name
    foreach ($asset in $result.assets)
    {
        if ($asset.name -match $package.asset_regex)
        {
            $urls.Add($asset.browser_download_url) | Out-Null
        }
    }
}
else
{
    $update_found = $false
}
",
  "asset_regex": "BraveBrowserStandaloneSilentDevSetup.exe$",
  "is_prerelease": true,
  "version_method": null,
  "custom_script": "b/bravesoftware.bravebrowser-dev.ps1",
  "skip": false
}
